<%= semantic_form_for @reim_form_message do |f| %>
  <%= f.inputs do %>
    <%= f.input :reimbursement_form_id %>
    <%= f.input :message %>
    <%= f.input :status_id %>
    <%= f.input :employee_id %>
  <% end %>

  <%= f.actions do %>
    <%= f.action :submit, :as => :input %>
  <% end %>
<% end %>
