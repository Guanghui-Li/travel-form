<div class='nested-fields'>
    <%= f.inputs do %>
          <div class="form-group">
            <%= f.label :expense_type %>
            <%= f.collection_select :expense_type_id, ExpenseType.order('name ASC'), :id, :name, {id: :wish_expense_type}, {class: "form-control"} %>
          </div>
        
          <div class="form-group">
            <%= f.label :cost %>
            <%= f.number_field :cost, id: :wish_cost, class: "form-control"%>
          </div>
        <%= link_to_remove_association "Remove Wish", f %>
    <% end %>
</div>